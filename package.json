{
  "name": "my-auth-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "next dev --turbopack -p 3000",
    "dev:backend": "nodemon server/server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "npx prisma migrate dev --schema=server/prisma/schema.prisma",
    "db:generate": "npx prisma generate --schema=server/prisma/schema.prisma",
    "db:studio": "npx prisma studio --schema=server/prisma/schema.prisma",
    "db:seed": "node server/prisma/seed.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.4",
    "@prisma/client": "^5.20.0",
    "axios": "^1.7.3",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.441.0",
    "next": "15.3.3",
    "nodemailer": "^6.9.14",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.57.0",
    "react-hot-toast": "^2.5.2",
    "recharts": "^2.12.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "concurrently": "^8.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "nodemon": "^3.1.4",
    "prisma": "^5.20.0",
    "tailwindcss": "^4"
  }
}
